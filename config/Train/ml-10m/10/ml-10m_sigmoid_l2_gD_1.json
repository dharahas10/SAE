{
    "dataset": "./processed_data/ml-100k/ml-100k-5.p",
    "name": "ml-100k-5",
    "seed": 0,
    "hidden_neurons": [700],
    "isMulti": false,
    "epochs": 3,
    "batch_size": 25,
    "normalization": 0,
  
    "regularization": {
      "l2_regularization": true,
      "l2_beta": 0.01
    },
  
    "dropout": {
      "bool": false,
      "keep_prob": 0.5
    },
  
    "noise": {
      "isNoisy": true,
      "noiseRatio": 0.1,
      "alpha": 0.7,
      "beta": 0.3
    },
  
    "optimizer": {
      "name": "gradientdescentoptimizer",
      "learning_rate": 0.001,
      "decay": true,
      "decay_steps": 1,
      "decay_rate": 0.96
    },
  
    "save_model": {
      "path": "./saved_models/ml-100k/5/",
      "name": "ml-100k-5.ckpt"
    }
  }